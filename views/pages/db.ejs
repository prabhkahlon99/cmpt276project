<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="container">
<h2>Database Results</h2>

<ul>
  <table border="2px">
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>E-mail</th>
      <th>Password</th>
    </tr>
    <% rows.forEach(function(r) { %>
      <tr>
        <td><%= r.id %></td>
        <td><%= r.name %></td>
        <td><%= r.email %></td>
        <td><%= r.password %></td>
      </tr><br>
    <% }); %>

  </table>
</ul

</div>

</body>
</html>
