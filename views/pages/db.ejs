<!DOCTYPE html>
<html>
<head>

</head>

<body>



<div class="container">
<h2>Database</h2>

<ul>
  <table border="2px">
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>E-mail</th>
      <th>Type</th>
      <th>Password</th>
    </tr>
    <% rows.forEach(function(r) { %>
      <tr>
        <td><%= r.id %></td>
        <td><%= r.name %></td>
        <td><%= r.email %></td>
        <td><%= r.type %></td>
        <td><%= r.password %></td>
      </tr><br>
    <% }); %>

  </table>
</ul

</div>

</body>
</html>
